{
  "name": "hostego-print-desktop",
  "version": "1.0.0",
  "description": "Hostego Print Desktop Application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "clean": "node scripts/clean-dist.js",
    "prebuild": "node scripts/clean-dist.js",
    "build": "electron-builder",
    "build:win": "node scripts/clean-dist.js && electron-builder --win --x64"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "build": {
    "appId": "com.hostego.print",
    "productName": "Hostego Print",
    "asar": true,
    "mac": {
      "icon": "assets/icon.icns",
      "target": ["dmg", "zip"]
    },
    "win": {
      "target": "nsis",
      "icon": "assets/favicon.ico"
    },
    "linux": {
      "icon": "assets/icon.png"
    }
  },
  "devDependencies": {
    "electron": "22.3.27",
    "electron-builder": "^26.4.0"
  }
}
